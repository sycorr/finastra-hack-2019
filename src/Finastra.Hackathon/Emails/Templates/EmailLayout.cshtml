            font-weight: bold;
            line-height: 30px;
        }

        .email-heading {
            border-bottom: 1px solid #ddd;
            padding-bottom: 4px;
            margin-bottom: 20px;
        }

        #templatePreheader {
            background-color: #eeeeee;
            border-top: 0;
            border-bottom: 0;
        }

        .preheaderContainer .mcnTextContent, .preheaderContainer .mcnTextContent p {
